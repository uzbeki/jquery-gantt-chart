{
  "name": "@uzbeki/jquery-gantt-chart",
  "version": "0.2.2",
  "description": "Simple Gantt Chart implemented with modernJS and jQuery. Previously known as jQuery.Gantt.",
  "main": "./dist/jquery.gantt.min.js",
  "engines": {
    "node": ">= 10.0.0"
  },
  "directories": {
    "test": "tests"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 0",
    "minify-js": "terser src/*.js -o dist/jquery.gantt.min.js --ecma 6 --compress --mangle --source-map",
    "minify-css": "cleancss src/*.css -o dist/jquery.gantt.min.css -O2",
    "minify": "npm run minify-js && npm run minify-css"
  },
  "repository": "https://github.com/uzbeki/jquery-gantt-chart.git",
  "keywords": [
    "Gantt",
    "Gantt Chart",
    "Javascript Gantt Chart",
    "jQuery",
    "ecosystem:jquery",
    "jquery-plugin"
  ],
  "author": "Bekhruz Otaev <behruzbekotayev@gmail.com> (https://bekhruz.com)",
  "contributors": [],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/uzbeki/jquery-gantt-chart/issues"
  },
  "homepage": "https://github.com/uzbeki/jquery-gantt-chart#readme",
  "devDependencies": {
    "clean-css-cli": "^5.6.3",
    "terser": "^5.29.2"
  }
}
